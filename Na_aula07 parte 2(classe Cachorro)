public class Cachorro  extends Animal implements Corrida {
    public Cachorro(String nome, int idade) {
        super(nome, idade);
    }

    @Override
    public String toString() {
        return "Cachorro{" +
                "nome='" + nome + '\'' +
                ", idade=" + idade +
                '}';
    }

    @Override
    public void som() {
        System.out.println("Au au");
    }

    @Override
    public void correr() {
        System.out.println("Correndo alegre!");
    }
}
